{
  "manifest_version": 3,
  "name": "Bookmark remover",
  "description": "Removes bookmark with hotkey",
  "version": "1.0",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "action": {},
  "background": {
    "service_worker": "service_worker.js",
    "type": "module"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "windows": "Alt+B",
        "mac": "Alt+B",
        "chromeos": "Alt+B",
        "linux": "Alt+B"
      }
    }
  },
  "permissions": [
    "activeTab",
    "commands",
    "bookmarks"
  ]
}